    "lodash": "^4.15.0",
    "numeral": "^1.5.3",
    "react": "^15.3.1",
    "react-addons-shallow-compare": "^15.3.1",
    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.3.1",
    "react-virtualized": "^7.19.4",
    "request": "^2.74.0",
    "socket.io-client": "^1.4.8",
    "source-sans-pro": "^2.0.10",
    "superagent": "^2.2.0",
